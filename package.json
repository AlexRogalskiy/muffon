{
  "name": "muffon-desktop",
  "version": "0.1.0",
  "description": "muffon-desktop",
  "license": "MIT",
  "homepage": "./",
  "author": {
    "name": "staniel359",
    "email": "staniel359@gmail.com",
    "url": "https://github.com/staniel359"
  },
  "main": "src/electron.js",
  "scripts": {
    "webpack-start": "webpack serve",
    "electron-start": "electron .",
    "wait-electron-start": "wait-on http://localhost:3000 && yarn electron-start",
    "start": "concurrently \"yarn webpack-start\" \"yarn wait-electron-start\"",
    "test": "jest",
    "format": "prettier . -w",
    "lint": "eslint . --fix",
    "brush": "yarn format && yarn lint",
    "webpack-build": "webpack",
    "electron-build": "electron-builder --win",
    "build": "yarn package --dir",
    "package": "yarn webpack-build && yarn electron-build"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.14.0",
    "style-loader": "^2.0.0",
    "wait-on": "^5.2.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "private": true,
  "build": {
    "appId": "com.electron.muffon-desktop",
    "copyright": "Copyright Â© 2020 staniel359",
    "mac": {
      "category": "public.app-category.music"
    },
    "linux": {
      "category": "Audio"
    },
    "compression": "store",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist",
      "src/electron.js"
    ]
  },
  "postinstall": "electron-builder install-app-deps",
  "proxy": "http://localhost:3001/"
}
