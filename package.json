{
  "name": "muffon-desktop",
  "version": "0.1.0",
  "description": "muffon",
  "license": "MIT",
  "homepage": "./",
  "author": {
    "name": "staniel359",
    "email": "staniel359@gmail.com",
    "url": "https://github.com/staniel359"
  },
  "main": "src/electron.js",
  "scripts": {
    "webpack-start": "webpack serve",
    "ws": "yarn webpack-start",
    "electron-start": "electron .",
    "es": "yarn electron-start",
    "wait-electron-start": "wait-on http://localhost:3000 && yarn electron-start",
    "start": "concurrently \"yarn webpack-start\" \"yarn wait-electron-start\"",
    "test": "jest",
    "format": "prettier . -w",
    "lint": "eslint \"**/*.{js,jsx}\" --fix",
    "brush": "yarn format && yarn lint",
    "webpack-build": "webpack",
    "electron-build": "electron-builder --win",
    "build": "yarn package --dir",
    "package": "yarn webpack-build && yarn electron-build"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "deepmerge": "^4.2.2",
    "electron-is-dev": "^1.2.0",
    "mousetrap": "^1.6.5",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.27.13",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "slick-carousel": "^1.8.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.12.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unused-imports": "^1.0.0",
    "file-loader": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^2.1.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "private": true,
  "build": {
    "appId": "com.electron.muffon",
    "copyright": "Copyright Â© 2020 staniel359",
    "win": {
      "icon": "dist/icons/icon.ico"
    },
    "mac": {
      "category": "public.app-category.music"
    },
    "linux": {
      "category": "Audio"
    },
    "compression": "store",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist",
      "src/electron.js"
    ]
  },
  "postinstall": "electron-builder install-app-deps"
}
